{
    "manifest_version": 3,
    "name": "ShortTab",
    "description": "adds shortcuts for your tabs",
    "version": "1.0",
    "icons": {
      "16": "",
      "32": "",
      "48": "",
      "128": ""
    },
    "action": {
        "default_title": "ShortTab",
        "default_popup": "index.html"
    },
    "content_scripts": [
    {
        "matches": ["*"],
        "css": ["style.css"],
        "js": ["script.js"]

    }
]
  }